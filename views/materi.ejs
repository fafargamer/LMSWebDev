              <tbody>
                <tr class="border">




                  <% data.forEach(function(item, i) { %>
                  <!-- <?php var_dump($data) ?> -->
                  <!--foreach-->
                    <tr>                   
                      <td><%=(i+1)%></td>
                      <td><%=item.filename%></td>
                      <td><%=item.mataPelajaran%></td>
                      <td><%=item.tanggalUnggah%></td>
                      <td><%=item.totalPoin%></td>
